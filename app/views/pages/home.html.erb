                              <% end %>
                            <% end %>
                          <% end %>

                          <select id="index-dept" data-minimum-results-for-search="Infinity" class="form-control", style="background-color: white">
                            <% result = result.sort %>
                            <% result.each do |dept| %>
                              <% if dept == "Aisne" %>
                                <% @valuedept = "Aisne" %>
                                <option value="Aisne"><%= dept %></option>
                                <% elsif dept == "Nord" %>
                                <% @valuedept = "Nord" %>
                                <option value="Nord"><%= dept %></option>
                                <% elsif dept == "Oise" %>
                                <% @valuedept = "Oise" %>
                                <option value="Oise"><%= dept %></option>
                                <% elsif dept == "Pas-de-Calais" %>
                                <% @valuedept = "Pas-de-Calais" %>
                                <option value="Pas-de-Calais"><%= dept %></option>
                                <% elsif dept == "Somme" %>
                                <% @valuedept = "Somme" %>
                                <option value="Somme"><%= dept %></option>
                                <% else %>
                                <% @valuedept = 0 %>
                                <option value=""><%= dept %></option>
                              <% end %>
                            <% end %>
                          </select>


                          <input type="button" onclick="getOption()" value="Valider!">
                          <script>
                            function getOption() {
                            const city = document.getElementById("index-city");
                            const deptdept = document.getElementById("index-dept").value;
                            city.innerHTML = "";

                            fetch(`/citywithdept?dept=${deptdept}`)
                              .then(response => response.json())
                              .then((data) => {
                            console.log(data);
                            data.forEach((person) => {
                            const item = `<option>${person.city}</option>` ;
                            city.insertAdjacentHTML("beforeend", item);
                              });
                              });


                            };

                            function getOption2() {
                            const name = document.getElementById("index-doctor");
                            const citycity = document.getElementById("index-city").value;
                            name.innerHTML = "";

                            fetch(`/dentistwithcity?city=${citycity}`)
                              .then(response => response.json())
                              .then((data) => {
                            console.log(data);
                            data.forEach((person) => {
                            const item = `<option>${person.first_name} ${person.last_name}</option>` ;
                            name.insertAdjacentHTML("beforeend", item);
                              });
                              });

                            };


