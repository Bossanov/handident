                <form data-form-output="form-output-global" data-form-type="contact" method="post" action="bat/rd-mailform.php" novalidate="novalidate" class="rd-mailform offset-top-24">
                  <div class="group-12 group-top">
                    <div class="group-item element-fullwidth">
                      <div class="form-group form-group-label-outside">
                        <label for="index-name" class="form-label form-label-outside rd-input-label text-white-06">Département</label>
                        <div class="select2-whitout-border shadow-drop-md">
                          <% @profiles = Profile.all %>
                          <% result = [] %>
                          <% @profiles.each do |pro| %>
                            <% if pro.category == "Dentiste" %>
                              <% until result.include?(pro.departement) %>
                                <% result << pro.departement %>
                              <% end %>
                            <% end %>
                          <% end %>

                          <select id="index-dept" data-minimum-results-for-search="Infinity" class="form-control", style="background-color: white">
                            <option value=""><%= %></option>
                            <% excl_nil_result = result.reject{|s| s.to_s == ''}  %>
                            <% if excl_nil_result[0].nil?  %>
                            <% else  %>
                              <% excl_nil_result = excl_nil_result.sort %>
                              <% excl_nil_result.each do |dept| %>
                                <% if dept == "Aisne" %>
                                  <% @valuedept = "Aisne" %>
                                  <option value="Aisne"><%= dept %></option>
                                  <% elsif dept == "Nord" %>
                                  <% @valuedept = "Nord" %>
                                  <option value="Nord"><%= dept %></option>
                                  <% elsif dept == "Oise" %>
                                  <% @valuedept = "Oise" %>
                                  <option value="Oise"><%= dept %></option>
                                  <% elsif dept == "Pas-de-Calais" %>
                                  <% @valuedept = "Pas-de-Calais" %>
                                  <option value="Pas-de-Calais"><%= dept %></option>
                                  <% elsif dept == "Somme" %>
                                  <% @valuedept = "Somme" %>
                                  <option value="Somme"><%= dept %></option>
                                  <% else %>
                                  <% @valuedept = 0 %>
                                  <option value=""><%= dept %></option>
                                <% end %>
                              <% end %>
                            <% end  %>
                          </select>



                          <script>
                            const selectdept= document.getElementById("index-dept");
                            selectdept.addEventListener("change", (event) => {
                              const city = document.getElementById("index-city");
                              const deptdept = document.getElementById("index-dept").value;
                              city.innerHTML = "";

                              fetch(`/citywithdept?dept=${deptdept}`)
                                .then(response => response.json())
                                .then((data) => {
                                  data.forEach((person) => {
                                  const item = `<option id="${person.city}" value="${person.city}">${person.city}</option>` ;
                                  city.insertAdjacentHTML("beforeend", item);
                                  });
                                });



                            city.addEventListener("change", (event) => {
                              const boutton = document.getElementById("bouttonsearch");
                              const name = document.getElementById("index-doctor");
                              const citycity = document.getElementById("index-city").value;
                              name.innerHTML = "";
                              fetch(`/dentistwithcity?city=${citycity}`)
                                .then(response => response.json())
                                .then((data) => {

                                  data.forEach((person) => {
                                    const item = `<option value ="${person.id}">${person.first_name} ${person.last_name}</option>` ;
                                    name.insertAdjacentHTML("beforeend", item);
                                  });
                                });
                                while (boutton.firstChild) {
                                  boutton.removeChild(boutton.firstChild);
                                };
                              boutton.insertAdjacentHTML("beforeend", '<button type="button" onclick="getOption3()" class="btn-fullwidth btn-ellipse btn btn-primary">Afficher</button>');
                              });
                            name.innerHTML = "";
                            });
                            function getOption3() {
                            const citycity = document.getElementById("index-city").value;
                            const deptdept = document.getElementById("index-dept").value;
                            const nameid = document.getElementById("index-doctor").value;
                            document.location.href=`/getSearch?id=${nameid}&city=${citycity}&dept=${deptdept}`;


                            };
                          </script>

